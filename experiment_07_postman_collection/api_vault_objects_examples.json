  "common_field_types": {
    "description": "Common field types found in Vault Object metadata",
    "types": {
      "ID": "System-generated unique identifier",
      "String": "Text field with max_length property",
      "Number": "Numeric field with min_value, max_value, scale properties",
      "Date": "Date field in YYYY-MM-DD format",
      "DateTime": "DateTime field in ISO 8601 format",
      "Boolean": "True/false field",
      "Picklist": "Dropdown selection from predefined values",
      "Object": "Reference to another object (lookup field)",
      "LongText": "Multi-line text field",
      "RichText": "Formatted text with HTML support",
      "Currency": "Monetary value with currency code",
      "Attachment": "File attachment field (max 100MB)"
    }
  },
  "standard_fields": {
    "description": "Standard fields present on most Vault objects",
    "fields": {
      "id": "System-generated unique record identifier",
      "name__v": "Primary name field (may be system-managed)",
      "status__v": "Record status (e.g., active__v, inactive__v)",
      "created_by__v": "User ID who created the record",
      "created_date__v": "DateTime when record was created",
      "modified_by__v": "User ID who last modified the record",
      "modified_date__v": "DateTime when record was last modified",
      "external_id__v": "User-defined external identifier (unique)",
      "object_type__v": "Object type ID for typed objects",
      "state__v": "Lifecycle state (for lifecycle-enabled objects)",
      "global_id__sys": "System-wide unique identifier"
    }
  },
  "response_status_values": {
    "SUCCESS": "Request executed successfully with no warnings",
    "WARNING": "Request executed with one or more warnings (e.g., no-op on update)",
    "FAILURE": "Request failed to execute (e.g., invalid sessionId, invalid data)"
  },
  "naming_conventions": {
    "__v": "Veeva-provided standard objects and fields (e.g., product__v, name__v)",
    "__c": "Customer-created custom objects and fields (e.g., campaign__c, revenue__c)",
    "__sys": "System objects and fields (e.g., user__sys, global_id__sys)",
    "__vr": "Outbound reference relationship name suffix",
    "__cr": "Custom relationship name suffix"
  }
